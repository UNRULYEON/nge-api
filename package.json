{
  "name": "nge-api",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "db:seed": "tsx src/db/seed/index.ts",
    "db:generate": "pnpx prisma generate --schema src/db/schema.prisma",
    "db:push": "pnpx prisma db push --schema ./src/db/schema.prisma",
    "db:pull": "pnpx prisma db pull --schema ./src/db/schema.prisma",
    "db:reset": "pnpx prisma migrate reset --schema ./src/db/schema.prisma",
    "db:deploy": "pnpx prisma migrate deploy --schema ./src/db/schema.prisma",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@hono/node-server": "^1.13.2",
    "@prisma/client": "^5.21.1",
    "hono": "^4.6.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "prettier": "3.3.3",
    "tsx": "^4.7.1"
  }
}
