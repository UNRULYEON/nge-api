generator client {
  binaryTargets   = ["native", "darwin-arm64"]
  provider        = "prisma-client-js"
  previewFeatures = ["deno"]
  output          = "generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Person {
  id   String @id
  name String
}
